	echo "lower_False_strip_False_token_False_charLen_5.output" >> output_counts.second
	diff lower_False_strip_False_token_False_charLen_5.output input.correct.txt | /usr/xpg4/bin/grep -E '^>' | wc -l | tee -a output_counts.second

clean:
	rm -f output_counts.first
	rm -f output_counts.second

compile:
	rm -f output_counts
	cat output_counts.first output_counts.second > output_counts
